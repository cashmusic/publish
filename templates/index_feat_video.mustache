{{{header}}}
<!--SITE HEADER-->

<main id="main" class="article-page static pure-g" role="main">
<article class="pure-u-1 expanded">
  <div id="hash-hero" role="hero">
      <!--h2 class="watt-logo"><svg version="1.1" id="watt-logo" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="183.822px" height="175.824px" viewBox="0 0 183.822 175.824" enable-background="new 0 0 183.822 175.824" xml:space="preserve"><g><path class="watt-logo-inner" fill="#FFFFFF" d="M183.823,175.824H0V0h183.822L183.823,175.824L183.823,175.824z M14,161.824h155.822V14H14V161.824z"/></g><g><path class="watt-logo-inner"  fill="#FFFFFF" d="M40.208,30.261l7.048,32.597l7.901-32.597h10.321l7.617,32.733l7.333-32.733h13.597l-13.954,50.15H68.255L60.353,47.95L52.31,80.411H40.137L26.54,30.261H40.208z"/></g><g><path class="watt-logo-inner"  fill="#FFFFFF" d="M119.102,72.159l-2.776,7.251h-14.45l21.144-50.149h10.605l20.931,50.149H139.96l-2.562-7.251H119.102zM122.803,61.994h11.033l-5.409-17.146h-0.215L122.803,61.994z"/></g><g><path class="watt-logo-inner" fill="#FFFFFF" d="M51.882,109.834H31.489v-11.25h54.17v11.25H65.337v38.898H51.882V109.834z"/></g><g><path class="watt-logo-inner"  fill="#FFFFFF" d="M121.4,109.834h-20.395v-11.25h54.17v11.25h-20.321v38.898H121.4V109.834z"/></g></svg></h2-->
      <h2 class="watt-welcome">Welcome to WATT</h2>
      <h3>We've been building free, open source tools directly with artists for years but to truly empower musicians it is vital that we pair those tools with educational resources. The combination of tools and learning will truly make for a sustainable future for musicians.</h3>
      <div class="mouse"></div><!--mouse-->
  </div><!--hero-->
  <section class="article-content">

    {{#featured_work}}
    <div class="pure-u-1 featured-work {{type}}">
        <div class="featured-banner {{template_blend}} {{template_bg_style}} {{template_additional}}" style="background-image:url('{{#assets}}{{url}}{{/assets}}');">
            <h2><a href="/{{type}}/{{id}}">{{title}}</a></h2>
              <h3>{{description}}</h3>
      <a class="more-link" href="/{{type}}/{{id}}">Read more</a>
        </div><!--featured-banner-->
    </div><!--featured-work-->
        {{/featured_work}}

<section class="pure-u-1">
<h3>Featured articles:</h3>
</section>
<div id="archive" class="pure-u-1">
<section class="pure-u-1">
  {{#secondary_work}}<div class="article-archive pure-u-1 pure-u-md-1-2 secondary {{template_additional}} {{type}}">
    <!--If Video embed present-->
    {{#assets}}{{#embed}}
    <div class="video_inner">
      {{{embed}}}
    </div><!--video_inner-->
    {{/embed}}{{/assets}}
    <div id="{{author_id}}" class="inner {{template_blend}}" style="background-image:url('{{#assets}}{{url}}{{/assets}}');">
        <h2><a href="/{{type}}/{{id}}">{{title}}</a></h2>
        <h4><a href="/author/{{author_id}}">{{author_name}}</a></h4>
          <p>{{description}}</p>
        <a class="more-link" href="/{{type}}/{{id}}">Read more</a>
      </div><!--inner--></div><!--article-archive-->{{/secondary_work}}{{#tertiary_work}}
<div class="article-archive pure-u-1 pure-u-md-1-4 {{template_additional}} {{type}}">
  <a class="wrap-link" href="/{{type}}/{{id}}"><div class="sixteen-nine">
      <div class="sn-content" style="background-image:url('{{#assets}}{{url}}{{/assets}}');">
    </div><!--sn-content-->
  </div><!--sixteen-nine--></a><!--wrap-link-->
        <div id="{{author_id}}" class="inner">
          <div class="corner-marker"></div><!--important-->
				          <h2><a href="/{{type}}/{{id}}">{{title}}</a></h2>
				 	        <h4><a href="/author/{{author_id}}">{{author_name}}</a></h4>
                  <p>{{description}}</p>
                  <a class="more-link" href="/{{type}}/{{id}}">Read more</a>
        </div><!--inner-->
	</div><!--article-archive-->{{/tertiary_work}}
</section>
</div><!--archive-->
<section class="article-archive pure-u-1 featured-video">
{{#featured_video}}
<div class="featured-video-content">
  {{#assets}}{{#embed}}
  <div class="video_inner">
    {{{embed}}}
  </div><!--video_inner-->
  {{/embed}}{{/assets}}
{{/featured_video}}
</div><!--featured-video-->
</section>
<section class="pure-u-1 quaternary space-out">
  {{#quaternary_work}}<div class="article-archive pure-u-1 pure-u-md-1-4 quaternary">
    <div class="quaternary-outer"  style="background-image:url('{{#assets}}{{url}}{{/assets}}');">
      <div class="inner">
        <h2><a href="/{{type}}/{{id}}">{{title}}</a></h2>
          <h4><a href="/author/{{id}}">Written by: {{author_name}}</a></h4>
        </div><!--inner-->
    </div><!--quarternary-->
  </div>{{/quaternary_work}}
</section>

<section class="pure-u-1 pure-u-md-1-2">
    <h3>Featured tags:</h3>
    <ul class="tags">
  {{#featured_tags}}
    <li><a class="tag" href="/tag/{{.}}">{{.}}</a></li>
  {{/featured_tags}}
</ul>
</section><section class="pure-u-1 pure-u-md-1-2">
    <h3>Featured authors:</h3>
    <ul class="tags">
  {{#featured_authors}}
    <li><a class="tag" href="/author/{{.}}">{{.}}</a></li>
       <li><a class="tag" href="/author/{{title}}">{{name}}</a></li>
  {{/featured_authors}}
</ul>
</section>
</section><!--article-content-->
</article>

</main><!--#main-->

<!--SITE FOOTER-->
{{{footer}}}
